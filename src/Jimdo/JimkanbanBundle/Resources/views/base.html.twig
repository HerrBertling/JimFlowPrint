<html>
    <head>
        {% stylesheets filter='cssrewrite'
            'bundles/jimdojimkanban/css/backend/*'
        %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}


        {% javascripts
            '@JimdoJimkanbanBundle/Resources/public/js/jquery.js'
            '@JimdoJimkanbanBundle/Resources/public/js/backend/*'
        %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

        <link href='http://fonts.googleapis.com/css?family=Roboto:100,400,700' rel='stylesheet' type='text/css' />

    </head>
    <body>

        <div id="content">
            <navigation>
               {% include 'JimdoJimkanbanBundle::navigation.html.twig' %}
            </navigation>
            {% block body %}{% endblock %}
        </div>
    </body>

</html>
